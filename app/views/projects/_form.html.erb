 <%= simple_form_for(@project) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :detail %>
  <%= f.input :details_01 %>
  <%= f.input :details_02 %>
  <%= f.input :details_03%>
  <%= f.input :details_04 %>
  <%= f.input :owner %>
  <%= f.input :main_contractor %>
  <%= f.input :year_completion %>
  <%= f.input :duration %>
  <%= f.input :labor_force %>
  <%= f.input :latitude %>
  <%= f.input :longitude %>
  <%= f.label :images%>
  <%= f.file_field :images, multiple: true %>
  <%= f.label :PDF %>
  <%= f.file_field :pdf %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>

<%= @project.errors[:images] %>
